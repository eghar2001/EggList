{
  "providers": [],
  "buildImage": "ghcr.io/railwayapp/nixpacks:ubuntu-1705968199",
  "variables": {
    "NIXPACKS_METADATA": "python",
    "PYTHONUNBUFFERED": "1"
  },
  "phases": {
    "install": {
      "dependsOn": [
        "setup"
      ],
      "cmds": [
        "sudo apt-get install python3-dev default-libmysqlclient-dev build-essential ",
        "python -m venv --copies /opt/venv && . /opt/venv/bin/activate && pip install --upgrade pip && pip install -r requirements.txt"
      ],
      "cacheDirectories": [
        "/root/.cache/pip"
      ],
      "paths": [
        "/opt/venv/bin"
      ]
    },
    "setup": {
      "nixPkgs": [
        "python310",
        "gcc"
      ],
      "nixLibs": [
        "zlib",
        "stdenv.cc.cc.lib"
      ],
      "nixOverlays": [],
      "nixpkgsArchive": "5148520bfab61f99fd25fb9ff7bfbb50dad3c9db"
    },
    "start": {
      "start_command": "flask --app EggList run "
    }
  }
}
